{
  "manifest_version": 2,
  "name": "Rabbithole",
  "description": "Find your way around the net",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self'; default-src 'self';",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": ["storage", "tabs"],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "popup/index.html",
    "default_title": "Rabbithole"
  }
}
